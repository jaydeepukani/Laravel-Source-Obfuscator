{
  "name": "jaydeepukani/laravel-source-obfuscator",
  "keywords": [
    "laravel",
    "source",
    "encrypt",
    "encryption",
    "obfuscator",
    "obfuscate",
    "phpBolt"
  ],
  "type": "library",
  "require": {
    "php": ">=7.2|>=8.0",
    "illuminate/console": "^6|^7|^8|^9|^10|^11",
    "illuminate/support": "^6|^7|^8|^9|^10|^11",
    "ext-bolt": "*"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "description": "Laravel Source Obfuscator",
  "license": "MIT",
  "authors": [
    {
      "name": "Jaydeep Ukani",
      "email": "jaydeepukani@outlook.in"
    }
  ],
  "autoload": {
    "psr-4": {
      "JaydeepUkani\\LaravelSourceObfuscator\\": "src"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "JaydeepUkani\\LaravelSourceObfuscator\\SourceEncryptServiceProvider"
      ]
    }
  }
}